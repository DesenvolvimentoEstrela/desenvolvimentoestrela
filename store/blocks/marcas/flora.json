      "flex-layout.row#promocoes-flora",
      "flex-layout.row#lancamentos-flora",
      "flex-layout.row#melhores-produtos-flora",
      "flex-layout.row#ofertas-flora",
      "flex-layout.row#mais-vendidos-flora"
    ],
    "parent": {
      "header": "header#flora",
      "footer": "footer#flora"
    }
  },
  "flex-layout.row#banner-principal-flora": {
    "children": [
      "list-context.image-list#banner-principal-flora"
    ],
    "props": {
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "banner-principal",
      "title": "Banner Principal"
    }
  },
  "list-context.image-list#banner-principal-flora": {
    "children": [
      "slider-layout#banner-principal-flora"
    ],
    "props": {
      "height": "493px",
      "images": [
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        },
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        },
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        }
      ]
    }
  },
  "slider-layout#banner-principal-flora": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showPaginationDots": "mobileOnly",
      "showNavigationArrows": "always",
      "blockClass": "banner-principal",
      "autoplay": {
        "timeout": "4000",
        "stopOnHover": true
      }
    }
  },
