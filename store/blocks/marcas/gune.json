      "footer": "footer#gune"
    }
  },
  "flex-layout.row#banner-principal-gune": {
    "children": [
      "list-context.image-list#banner-principal-gune"
    ],
    "props": {
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "banner-principal",
      "title": "Banner Principal"
    }
  },
  "list-context.image-list#banner-principal-gune": {
    "children": [
      "slider-layout#banner-principal-gune"
    ],
    "props": {
      "height": "493px",
      "images": [
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        },
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        },
        {
          "image": "assets/estrela-lp-banner-full-provisorio.png",
          "mobileImage": "assets/aa.png"
        }
      ]
    }
  },
  "slider-layout#banner-principal-gune": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showPaginationDots": "mobileOnly",
      "showNavigationArrows": "always",
      "blockClass": "banner-principal",
      "autoplay": {
        "timeout": "4000",
        "stopOnHover": true
      }
    }
  },
