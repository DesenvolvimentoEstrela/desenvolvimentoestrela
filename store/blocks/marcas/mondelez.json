        "parent": {
            "header": "header#mondelez",
            "footer": "footer#mondelez"
        }
    },
    "flex-layout.row#banner-principal-mondelez": {
        "children": [
            "list-context.image-list#banner-principal-mondelez"
        ],
        "props": {
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "blockClass": "banner-principal",
            "title": "Banner Principal"
        }
    },
    "list-context.image-list#banner-principal-mondelez": {
        "children": [
            "slider-layout#banner-principal-mondelez"
        ],
        "props": {
            "height": "493px",
            "images": [
                {
                    "image": "assets/estrela-lp-banner-full-provisorio.png",
                    "mobileImage": "assets/aa.png"
                },
                {
                    "image": "assets/estrela-lp-banner-full-provisorio.png",
                    "mobileImage": "assets/aa.png"
                },
                {
                    "image": "assets/estrela-lp-banner-full-provisorio.png",
                    "mobileImage": "assets/aa.png"
                }
            ]
        }
    },
    "slider-layout#banner-principal-mondelez": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true,
            "showPaginationDots": "mobileOnly",
            "showNavigationArrows": "always",
            "blockClass": "banner-principal",
            "autoplay": {
                "timeout": "4000",
                "stopOnHover": true
            }
        }
    },
