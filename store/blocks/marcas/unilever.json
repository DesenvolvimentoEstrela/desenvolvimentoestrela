    "flex-layout.row#banner-principal-unilever": {
        "children": [
            "list-context.image-list#banner-principal-unilever"
        ],
        "props": {
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "blockClass": "banner-principal",
            "title": "Banner Principal"
        }
    },
    "list-context.image-list#banner-principal-unilever": {
        "children": [
            "slider-layout#banner-principal-unilever"
        ],
        "props": {
            "height": "493px",
            "images": [
                {
                    "image": "assets/seda.png",
                    "mobileImage": "assets/aa.png"
                },
                {
                    "image": "assets/cremogema.png",
                    "mobileImage": "assets/aa.png"
                },
                {
                    "image": "assets/confort.png",
                    "mobileImage": "assets/aa.png"
                }
            ]
        }
    },
    "slider-layout#banner-principal-unilever": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true,
            "showPaginationDots": "mobileOnly",
            "showNavigationArrows": "always",
            "blockClass": "banner-principal",
            "autoplay": {
                "timeout": "4000",
                "stopOnHover": true
            }
        }
    },
